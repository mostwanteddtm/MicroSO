				urls=urls.split('|');
				cookie.setcookie('easeusid1_'+urls[0],'1');
				cookie.setcookie('easeusid2_'+urls[1],'1');
				cookie.setcookie('easeusid3_'+urls[2],'1');
				cookie.setcookie('easeusid4_'+urls[3],'1');
				cookie.setcookie('easeusid5_'+urls[4],'1');
				cookie.setcookie('easeusid6_'+urls[5],'1');
			}
			return true;
		});
		$('.download_cookie').click(function(){
			if(!(/easeusiddown/.test(document.cookie))){
				var urls='easeusiddown_'+Number(new Date()).toString()+self.rd(1,100)+'|'+cookie.getcookie('easeus_url')+'|'+document.referrer+'|'+document.URL+'|'+$(this).attr('cname')+'|';
				urls=urls.replace(/https?\:\/\/www\.easeus\.com\//g,'/');
				cookie.setcookie(urls,'1');
				$.ajax({
					type: "POST",
					url: "/collect/",
					data:"url="+urls,
					cache: false
				});
			}
			return true;
		})
	};
    this.check_input=function(s,error,word,length,formail){
        s=s.replace(/\s*/,'');
        var pattern =/^[a-zA-Z0-9_\-.]{1,}@[a-zA-Z0-9_\-]{1,}\.[a-zA-Z0-9_\-.]{1,}$/;
        if(typeof(word)=='undefined'){
            word=false;
        }
        if(typeof(length)=='undefined'){
            length=0;
        }
        if(s!=""){
            if(s.length<length){
                error.removeClass('hidden');
                 if(word!=false){
                     error.html(word);
                 }
                return false;
            }else{
                if(formail=='mail'){
                    if(!pattern.exec(s)){
                        error.removeClass('hidden');
                        error.html(word);
                        return false;
                    }else{
                        error.addClass('hidden');
                        error.html(word);
                        return true;
                    }
                }else{
                    error.addClass('hidden');
                    error.html(word);
                    return true;
                }
             }
        }else{
            error.removeClass('hidden').html('Can\'t be empty');
            return false;
        }
    };
    this.ico_question_globle=function(clas){
        $('body').append('<div class="ioc_question_globle"></div>');
        var ioc_question_globle=$('.ioc_question_globle');
        $(clas).children().hide();
        $(clas+",.ioc_question_globle").mouseenter(function(){
            if($(this).hasClass(clas.replace('.',''))){
                var offsets=$(this).offset();
                ioc_question_globle.html($(this).text()).css({left:offsets.left-160,top:offsets.top+22});
            }
            ioc_question_globle.show()
        }).mouseleave(function(){
            ioc_question_globle.hide();
        })
    };
	this.bundle=function(_price,_url,_images,fh){//fh==符号
        fh=fh || '$';
        var self=this;
		var get_num=function(num){
			return (Number(num)+0.00001).toString().match(/.*\.\d{2}/)[0]
		}
		$('.list_bundle_plus .checkbox').click(function(){
			if($(this).hasClass('current')){
				$(this).removeClass('current');
				$(this).parent().find('.plus').addClass('gray');
				$(this).parent().removeClass('current');
			}else{
				$(this).addClass('current');
				$(this).parent().find('.plus').removeClass('gray');
				$(this).parent().addClass('current');
			}
            clicks();
        });
        function clicks(){
			var num=$('.list_bundle_plus .checkbox.current').length;
			if(num==0){
				var del_price=_price[0][0];
				var all_price=_price[0][1];
				var price0=_price[0][1];
				var price1=_price[1];
				var price2=_price[2];
				var url=_url[0];
				var img=_images[0];
				$('#word').html('Price for One:');
				$('.list_bundle_plus .tag_off').hide();
			}else if(num==1){
				if(Number($('.list_bundle_plus .checkbox.current').attr("value"))==1){
					var del_price=_price[0][0]+_price[1];
					var url=_url[1];
					var img=_images[1];
				}else{
					var del_price=_price[0][0]+_price[2];
					var url=_url[2];
					var img=_images[2];
				}
				var all_price=del_price*0.7;
				var price0=_price[0][0]*0.7;
				var price1=_price[1]*0.7;
				var price2=_price[2]*0.7;
				$('#word').html('Price for Both:');
				$('.list_bundle_plus .tag_off').show().addClass('off30');
			}else if(num==2){
				var del_price=_price[0][0]+_price[1]+_price[2];
				var all_price=del_price*0.6;
				var price0=_price[0][0]*0.6;
				var price1=_price[1]*0.6;
				var price2=_price[2]*0.6;
				var url=_url[3];
				var img=_images[3];
				$('#word').html('Price for All:');
				$('.list_bundle_plus .tag_off').show().removeClass('off30');
			}
			var save_price=del_price-all_price;
            $('#all_price').html('<font><sup>'+fh+'</sup>'+get_num(all_price).replace(/(\d*?)(\.\d*)/,'$1<sup>$2</sup>')+'</font>');
            $('#del_price').html(fh+get_num(del_price));
            $('#save_price').html('Save '+fh+get_num(save_price));
            $('#url').attr({href:url});
            $('.list_bundle_plus li:eq(0) .price').html('<del>'+fh+_price[0][0].toFixed(2)+'</del><font><sup>'+fh+'</sup>'+get_num(price0).replace(/(\d*?)(\.\d*)/,'$1<sup>$2</sup>')+'</font>');
            $('.list_bundle_plus li:eq(1) .price').html('<del>'+fh+_price[1].toFixed(2)+'</del><font><sup>'+fh+'</sup>'+get_num(price1).replace(/(\d*?)(\.\d*)/,'$1<sup>$2</sup>')+'</font>');
            $('.list_bundle_plus li:eq(2) .price').html('<del>'+fh+_price[2].toFixed(2)+'</del><font><sup>'+fh+'</sup>'+get_num(price2).replace(/(\d*?)(\.\d*)/,'$1<sup>$2</sup>')+'</font>');
            $('#img').html('<img alt="Product box of EaseUS Data Recovery Wizard Professional" src="'+img+'">');
			self.buy_linkid_run();//链接修改
		};
        clicks();
		this.height_resize('.list_bundle_plus > li');
	}
}
