	this.animate=function(clas,run_fun){
		$(clas).each(function(i){
			var this_top=$(this).offset().top-$(window).height()+100;
			var fun=function(event){
				var self=event.data.self;
                var this_top=self.offset().top-$(window).height()+100;
				if($(window).scrollTop()>this_top){
					self.addClass('run');
                    if(typeof(run_fun)=='function'){
                        run_fun(self);
                    }
					$(document).unbind('scroll',fun);
				}
			}
			$(document).bind('scroll',{self:$(this)},fun);
			//run onces
			if($(window).scrollTop()>this_top){
				$(this).addClass('run');
                if(typeof(run_fun)=='function'){
                    run_fun($(this));
                }
				$(document).unbind('scroll',fun);
			}
		});
